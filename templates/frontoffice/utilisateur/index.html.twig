<!DOCTYPE html>

<title>Utilisateur index</title>

{% block body %}
    <h1>Utilisateur index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Email</th>
                <th>Mot_de_passe</th>
                <th>Role</th>
                <th>Date_inscription</th>
                <th>Statut</th>
                <th>Specialite</th>
                <th>Preferences</th>
                <th>Privileges</th>
                <th>Twofa_secret_key</th>
                <th>Numtel</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for utilisateur in utilisateurs %}
            <tr>
                <td>{{ utilisateur.id }}</td>
                <td>{{ utilisateur.nom }}</td>
                <td>{{ utilisateur.email }}</td>
                <td>{{ utilisateur.motDePasse }}</td>
                <td>{{ utilisateur.role }}</td>
                <td>{{ utilisateur.dateInscription ? utilisateur.dateInscription|date('Y-m-d') : '' }}</td>
                <td>{{ utilisateur.statut }}</td>
                <td>{{ utilisateur.specialite }}</td>
                <td>{{ utilisateur.preferences }}</td>
                <td>{{ utilisateur.privileges }}</td>
                <td>{{ utilisateur.twofaSecretKey }}</td>
                <td>{{ utilisateur.numtel }}</td>
                <td>
                    <a href="{{ path('app_utilisateur_show', {'id': utilisateur.id}) }}">show</a>
                    <a href="{{ path('app_utilisateur_edit', {'id': utilisateur.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="13">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_utilisateur_new') }}">Create new</a>
{% endblock %}
